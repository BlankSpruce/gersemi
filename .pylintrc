[MASTER]
load-plugins=pylint.extensions.mccabe

[DESIGN]
max-complexity=10

[MESSAGES CONTROL]
disable=
    bad-continuation,

    duplicate-code,

    fixme,

    invalid-name,

    missing-class-docstring,
    missing-function-docstring,
    missing-module-docstring,

    no-self-use,

    too-few-public-methods,
    too-many-ancestors,

    unsubscriptable-object # false positives