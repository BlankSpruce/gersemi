cmake_minimum_required(VERSION 3.15)

cmake_minimum_required(VERSION 3.15 FATAL_ERROR)

cmake_minimum_required(VERSION 3.1...3.15)

cmake_minimum_required(VERSION 3.1...3.15 FATAL_ERROR)

cmake_policy(VERSION 3.15)

cmake_policy(VERSION 3.1...3.15)

cmake_policy(SET CMP0003 OLD)

cmake_policy(SET CMP0003 NEW)

cmake_policy(GET CMP0003 POLICY_TYPE)

cmake_policy(PUSH)

cmake_policy(POP)

get_cmake_property(FOO MACROS)

include_guard()

include_guard(DIRECTORY)

include_guard(GLOBAL)

# without default
option(FOO "help text")

# with default
option(FOO "help text" OFF)

return()

site_name(FOO)

# Unset normal variable
unset(FOO)

# in parent scope
unset(FOO PARENT_SCOPE)

# in cache
unset(FOO CACHE)

# in environment
unset(ENV{FOO})

variable_watch(FOO)

variable_watch(FOO some_command)
